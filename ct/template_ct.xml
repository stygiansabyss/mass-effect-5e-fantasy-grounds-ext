<?xml version="1.0" encoding="iso-8859-1"?>


<root>
    <template name="number_clientct_shields">
        <number_ct_static name="shields">
            <anchored to="rightanchor" width="30" height="20">
                <top />
                <right anchor="left" relation="relative" offset="-10" />
            </anchored>
            <min value="0" />
            <script>
                function onInit()
                    setColor("FF61FFF9");
                end
            </script>
        </number_ct_static>
    </template>
    <template name="number_clientct_tech_armor">
