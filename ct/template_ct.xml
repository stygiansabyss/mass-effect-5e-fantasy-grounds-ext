            <script>
                function onInit()
                    setColor("FFCAB9FF");
                end
            </script>
        </number_ct_static>
    </template>

    <template name="string_clientct_shields_status">
        <string_static name="shields_status">
            <anchored to="rightanchor" width="30" height="20">
                <top />
                <right anchor="left" relation="relative" offset="-10" />
            </anchored>
            <center />
            <script>
                function onInit()
                    setColor("FF61FFF9");
                end
            </script>
            <invisible />
        </string_static>
    </template>
